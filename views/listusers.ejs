<!DOCTYPE html>
<html>
<head>
    <title>List of Users</title>
</head>
<body>
    <h1>Registered Users</h1>

    <ul>
        <% users.forEach(function(user){ %>
            <li>
                <%= user.first %> <%= user.last %> - <%= user.email %>
            </li>
        <% }) %>
    </ul>

    <a href="/">Back to menu</a>
</body>
</html>
